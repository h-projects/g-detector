{
  "name": "g-detector",
  "version": "0.1.6",
  "description": "A powerful JavaScript library to detect the letter G",
  "scripts": {
    "build": "tsup",
    "check": "biome check",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix --unsafe",
    "test": "node --import tsx --test test/*.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "author": "Superchupu",
  "license": "ISC",
  "keywords": [
    "gasbot",
    "detector",
    "h"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-projects/g-detector.git"
  },
  "bugs": {
    "url": "https://github.com/h-projects/g-detector/issues"
  },
  "homepage": "https://github.com/h-projects/g-detector#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@types/node": "^22.1.0",
    "tsup": "^8.1.0",
    "tsx": "^4.15.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.3.0"
}
